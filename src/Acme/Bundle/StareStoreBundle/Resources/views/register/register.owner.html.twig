<!DOCTYPE html>
<html>
{% block header%}
    {{ include('AcmeBundleStareStoreBundle:header.html.twig',{'titulo':'Registro Administrador'})}}
{% endblock %}
<body>
<div class="wrap_body">
<div class="wrap_body2">
<div id="container">
{% block menu %}
    {{ include('AcmeBundleStareStoreBundle:menu:menu.admin.html.twig',{'name':name})}}
{% endblock %}
{% block categories %}
    {{include('AcmeBundleStareStoreBundle:categories.html.twig')}}
{% endblock %}
<div id="notification"></div>
<div class="wrap_content">

    <div id="content">  
        <div class="breadcrumb">
            <a href="#">Home</a>
            &raquo; <a href="#">Cuenta</a>
            &raquo; <a href="#">Registro</a>
        </div>
        <h1>Registro de administrador</h1>
        <form action="{{base_url}}/register/owner" method="post" enctype="multipart/form-data">
            <h2>Datos Personales</h2>
            <div class="content">
                <table class="form">
                    <tr>
                        <td><span class="required">*</span> Nombre:</td>
                        <td><input type="text" name="nombre" value="" />
                        </td>
                    </tr>
                    <tr>
                        <td><span class="required">*</span> Apellido:</td>
                        <td><input type="text" name="apellido" value="" />
                        </td>
                    </tr>
                    <tr>
                        <td><span class="required">*</span> Correo:</td>
                        <td><input type="text" name="correo" value=""/>
                        </td>
                    </tr>
                    <tr>
                        <td><span class="required">*</span> Telefono:</td>
                        <td><input type="text" name="telefono" value=""/>
                        </td>
                    </tr>
                    <tr>
                        <td><span class="required">*</span> Celular:</td>
                        <td><input type="text" name="celular" value=""/>
                        </td>
                    </tr>
                </table>
            </div>
            <h2>Password</h2>
            <div class="content">
                <table class="form">
                    <tr>
                        <td><span class="required">*</span> Password:</td>
                        <td><input type="password" name="password" value="" />
                        </td>
                    </tr>
                    <tr>
                        <td><span class="required">*</span> Confirmar Password:</td>
                        <td><input type="password" name="confirm" value="" />
                        </td>
                    </tr>
                </table>
            </div>
            <div class="buttons">
                <div class="right">
                    <input type="submit" value="Continuar" class="button" />
                </div>
            </div>
        </form>
    </div>    
    
</div>
{% block footer%}
    {{ include('AcmeBundleStareStoreBundle:footer.html.twig')}}
{% endblock%}
</div>
</div>
</div>
</body>
</html>